CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE JOGO(CODIGOJOGO INTEGER,NOMEJOGO VARCHAR(250),PRECOJOGO DECIMAL(10,2),PLATAFORMAJOGO VARCHAR(150),GENEROJOGO VARCHAR(150),DESCRICAO VARCHAR(1000),PRECOVENDA DECIMAL(10,2),PRODUTORA VARCHAR(150))
CREATE MEMORY TABLE FUNCIONARIO(CODIGO INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,TELEFONE VARCHAR(20),NOME VARCHAR(250),RG VARCHAR(20),CPF VARCHAR(20),CARGO VARCHAR(150),SENHA INTEGER,IDADE INTEGER,SEXO BOOLEAN)
CREATE MEMORY TABLE CLIENTE(CODIGO INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,NOME VARCHAR(250),EMAIL VARCHAR(250),IDADE INTEGER,SEXO BOOLEAN,CPF VARCHAR(15),CEP INTEGER,TELEFONE VARCHAR(20))
ALTER TABLE FUNCIONARIO ALTER COLUMN CODIGO RESTART WITH 3
ALTER TABLE CLIENTE ALTER COLUMN CODIGO RESTART WITH 2
